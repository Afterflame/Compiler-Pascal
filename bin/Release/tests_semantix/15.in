program exFunction;
var
   a, b, ret : integer;
function max(num1, num2: integer): integer;
var
   resultt: integer;
begin
   if (num1 > num2) then
      resultt := num1
   else
      resultt := num2;
   max := resultt;
end;

begin
   a := 1;
   b := 1;
   repeat
   	ret := ret + max(a, b);
	a:=a+1
   until ret < 1000;
   write( 'TEXT');
end.